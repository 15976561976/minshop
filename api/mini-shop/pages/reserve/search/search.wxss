.search{
	display: flex;
	width: 690rpx;
	height: 80rpx;
	box-sizing: border-box;
	justify-content: space-between;
	margin: 20rpx auto;
	background: #fff;
	border-radius: 10rpx;
	
}
.search input{
	width: 550rpx;
	height: 80rpx;
	border-radius: 10rpx;
	padding: 0rpx 10rpx;
	box-sizing: border-box;
	background: #fff;
	font-size: 30rpx;
	line-height: 80rpx;
	margin-right: 20rpx;
}
.search button{
	width: 120rpx;
	height: 80rpx;
	background: #fff;
	line-height: 80rpx;
	color: #999;
	margin: 0;
}
.search button::after{
	border: none;
}
.search button span{
	
	font-size: 46rpx;
}
.context .search-title{
	font-size: 24rpx;
	color: #333;
	line-height: 60rpx;
	margin: 0rpx 30rpx;
	display: flex;
	justify-content: space-between;
}
.context .record{
	width: 100%;
	height: auto;
	min-height: 60rpx;
	display: flex;
	justify-content: flex-start;
	flex-wrap: wrap;
}
.context .record view{
	font-size: 24rpx;
	color: #333;
	line-height: 60rpx;
	margin: 10rpx 30rpx;
	background: #fff;
	border-radius: 10rpx;
	padding: 0 30rpx;
}
.context .result-tiele{
	font-size: 32rpx;
	color: #333;
	text-indent: 30rpx;
	line-height: 60rpx;
}
.staff,.type{
	width: 100%;
	height: 80rpx;
	background-color: #fff;
	line-height: 80rpx;
	border-bottom: 2rpx solid #ddd;
	padding: 0rpx 30rpx;
	box-sizing: border-box;
	color: #999;
	font-size: 30rpx;
	margin-top: 20rpx;
}
.ul .li:last-child{
	border-bottom: none;
}
.ul .typeLi:last-child{
	border-bottom: none;
}
.li{
	width: 100%;
	height: 146rpx;
	background-color: #fff;
	display: flex;
	justify-content: space-between;
	font-size: 30rpx;
	line-height: 40rpx;
	border-bottom: 2rpx solid #ddd;
	padding: 0rpx 30rpx;
	box-sizing: border-box;
	align-items: center;
}
.li .portrait{
	width: 90rpx;
	height: 90rpx;
	border-radius: 50%;
	overflow: hidden;
	background: #04BE01;
}
.li .portrait image{
	width: 100%;
	height: 100%;
}
.li .name{
	flex-grow:1;
	height: auto;
	padding-left: 30rpx;
}
.li .name text{
	color: #999;
}
.li .specialty{
	width: auto;
	height: auto;
	background: #ff4e67;
	color: #fff;
	padding: 0 10rpx;
}
.typeLi{
	width: 100%;
	height: 100rpx;
	background-color: #fff;
	display: flex;
	justify-content: space-between;
	line-height: 100rpx;
	border-bottom: 2rpx solid #ddd;
	padding: 0rpx 30rpx;
	box-sizing: border-box;
	align-items: center;
	font-size: 30rpx;
}
.typeLi .iconfont{
	font-size: 22rpx;
	color: #dcdcdc;
}
.load{
	width: 100%;
	height: 50rpx;
	line-height: 50rpx;
	font-size: 24rpx;
	text-align: center;
	color: #999;
}