/* pages/reserve/record/record.wxss */
.nav{
	width: 100%;
	height: 98rpx;
	border-top: 2rpx solid #ddd;
	background: #fff;
	display: flex;
	justify-content: space-around;
	line-height: 98rpx;
	color: #333;
	font-size: 30rpx;
	margin-bottom: 20rpx;
}
.nav text{
	width: 33.33%;
	text-align: center;
}
.list{
	width: 100%;
	height: 128rpx;
	border-bottom: 2rpx solid #ddd;
	display: flex;
	justify-content: space-between;
	font-size: 28rpx;
	background: #fff;
	align-items: center;
	text-align: center;
}
.list .name,.state,.time,.span,.cancel,.achieve{
	width: 25%;
}
.list .time{
	line-height: 36rpx;
	color: #999;
}
.list .span span{
	background: #ff4e67;
	padding: 0 10rpx;
	color: #fff;
}
.list .state{
	color: #999;
}
.list .cancel{
	color: #8f82bc;
}
.list .achieve{
	color: #22ac38;
}
.nomore {
  margin: 0 0 26rpx 0;
  display: flex;
  justify-content: center;
}
.nomore .nomore-text {
  font-size: 24rpx;
  color: #999999;
	line-height: 70rpx;
}